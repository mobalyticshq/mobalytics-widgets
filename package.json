{
  "name": "mobalytics-widgets",
  "version": "0.1.17",
  "description": "",
  "main": "dist/index.js",
  "umd:main": "dist/index.umd.js",
  "module": "dist/index.m.js",
  "source": "components/hello-world/index.js",
  "scripts": {
    "test": "NODE_ENV=test jest",
    "build": "preact build --template src/index.ejs --prerender false --sw false --esm false",
    "serve": "sirv build --port 8081 --cors --single",
    "dev": "preact watch --template src/index.ejs --prerender false --sw false --esm false",
    "upload": "./bin/upload-to-moba-cdn.sh",
    "eslint:base": "eslint --cache src/**/*.{ts,tsx}",
    "eslint:check": "yarn eslint:base --fix-dry-run",
    "eslint:fix": "yarn eslint:base --fix",
    "prettier:base": "prettier src/**/*.{ts,tsx}",
    "prettier:check": "yarn prettier:base --check",
    "prettier:fix": "yarn prettier:base --write",
    "style:check": "yarn prettier:check && yarn eslint:check",
    "style:fix": "yarn prettier:fix && yarn eslint:fix",
    "gql:lol:dynamic:download": "apollo service:download src/lol/api/dynamic/schema.json --endpoint 'https://widget.mobalytics.gg/lol/graphql/v1/query'",
    "gql:lol:dynamic:generate": "apollo codegen:generate --localSchemaFile=src/lol/api/dynamic/schema.json --includes=src/lol/api/dynamic/**/*.gql.ts --target typescript --passthroughCustomScalars --customScalarsPrefix=Gql --outputFlat src/lol/types/gql-dynamic",
    "gql:lol:static:download": "apollo service:download src/lol/api/static/schema.json --endpoint 'https://widget.mobalytics.gg/league/gql/static/v1'",
    "gql:lol:static:generate": "apollo codegen:generate --localSchemaFile=src/lol/api/static/schema.json --includes=src/lol/api/static/**/*.gql.ts --target typescript --passthroughCustomScalars --customScalarsPrefix=Gql --outputFlat src/lol/types/gql-static"
  },
  "keywords": [],
  "author": "Mobalytics.gg <hello@mobalyticshq.com>",
  "license": "MIT",
  "dependencies": {
    "clsx": "^1.1.1",
    "goober": "^2.0.38",
    "graphql": "^15.5.1",
    "graphql-hooks": "^5.2.0",
    "graphql-ws": "^5.3.0",
    "preact": "^10.3.4",
    "preact-compat": "^3.19.0",
    "preact-markdown": "^2.0.0",
    "subscriptions-transport-ws": "^0.9.19",
    "tippy.js": "^6.3.1"
  },
  "devDependencies": {
    "@types/enzyme": "^3.10.5",
    "@types/jest": "^26.0.23",
    "@types/ws": "^7.4.5",
    "@typescript-eslint/eslint-plugin": "^4.28.1",
    "@typescript-eslint/parser": "^4.28.1",
    "apollo": "^2.33.4",
    "enzyme": "^3.11.0",
    "enzyme-adapter-preact-pure": "^3.1.0",
    "eslint": "^7.29.0",
    "eslint-config-preact": "^1.1.4",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import-quotes": "^0.0.1",
    "husky": "^7.0.0",
    "jest": "^27.0.6",
    "jest-preset-preact": "^4.0.2",
    "lint-staged": "^11.0.0",
    "preact-cli": "^3.0.0",
    "prettier": "2.3.2",
    "sirv-cli": "^1.0.12",
    "typescript": "^4.3.4"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": "yarn prettier:fix"
  }
}
